Host *
  ServerAliveInterval 30
  ServerAliveCountMax 5

host github.com
  HostName github.com
  IdentityFile ~/.ssh/timnolte_id_rsa
  User git

host bitbucket.org
  HostName bitbucket.org
  IdentityFile ~/.ssh/timnolte_id_rsa
  User git

host gitlab.com
	HostName gitlab.com
	IdentityFile ~/.ssh/timnolte_id_rsa
	user git

Host heroku.com
 StrictHostKeyChecking no
